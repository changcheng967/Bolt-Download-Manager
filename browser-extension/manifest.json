{
  "manifest_version": 3,
  "name": "Bolt Download Manager",
  "version": "0.2.1",
  "description": "Integrate Bolt Download Manager with your browser for fast downloads",
  "permissions": [
    "downloads",
    "nativeMessaging",
    "contextMenus",
    "activeTab",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Bolt Download Manager"
  }
}
